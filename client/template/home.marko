<script>
    let setPageHeight = function(){
        let vh = window.innerHeight * 0.01;
        document.documentElement.style.setProperty('--vh', `${vh}px`);
    }
    setPageHeight();

    window.addEventListener('resize', setPageHeight);

    let getToday = function(){
        let today = new Date();
        let dd = String(today.getDate()).padStart(2, '0');
        let mm = String(today.getMonth() + 1).padStart(2, '0');
        let yyyy = today.getFullYear();

        return `${yyyy}-${mm}-${dd}`
    }

    document.getElementById("dateInAddNoShow").value = getToday();


    let sendAjax = function (url, data, fn){
        let xhr = new XMLHttpRequest();
        xhr.open('POST', url);
        xhr.setRequestHeader('Content-type', "application/json");
        xhr.send(JSON.stringify(data));

        // 데이터 수신이 완료되면 표시
        xhr.addEventListener('load', function(){
            fn(JSON.parse(xhr.responseText));
        });
    };

    let phoneNumberValidation = function(phone){
        const phoneRex = /^01([016789]?)([0-9]{3,4})([0-9]{4})$/;
        return phoneRex.test(phone);
    };
    let dateValidation = function(date){
        const dateRex = /([0-9]{8})$/;
        return dateRex.test(date);
    }

    let addTelListener = function(el, enterFn){
        el.onkeydown = function(e){
            let key = e.key;
            if (key === 'Enter' || key === 'Backspace') {
                return;
            }
            if(isNaN(key)){
                return false;
            }

            let str = el.value;
            let cleaned = ('' + str).replace(/\D/g, '');
            if (cleaned.length >= 11) {
                return false;
            }
        }
        el.onkeyup = function(e){
            if(e.key === "Enter"){
                if(enterFn){
                    enterFn();
                }
            }else{
                let str = el.value;
                let cleaned = ('' + str).replace(/\D/g, '').replace(/[^0-9]/gi,'');
                let formatted = '';
                let length = cleaned.length;
                if(length <= 3){
                    formatted = cleaned;
                }else if(length > 3 && length <= 6){
                    formatted = cleaned.substring(0,3) + '-' + cleaned.substring(3);
                }else if(length > 6 && length <= 10 ){
                    formatted = cleaned.substring(0,3) + '-' + cleaned.substring(3, 6) + '-' + cleaned.substring(6);
                }else{
                    formatted = cleaned.substring(0,3) + '-' + cleaned.substring(3, 7) + '-' + cleaned.substring(7);
                }
                el.value =  formatted;
            }
        };
    }

    let resetAllInput = function(){
        ['homeSearchNoShowContents', 'telInAddNoShow', 'noShowCaseEtc'].forEach( id => document.getElementById(id).value = null);
        document.getElementById("dateInAddNoShow").value = getToday();
        document.getElementById('chkAgree').checked = false;
        document.getElementById('reason1').checked = true;

        document.getElementById('homeSearchNoShowNotExist').classList.add('none');//이미지 숨김
        document.getElementById('homeSearchNoShowCountArea').style.visibility = 'hidden';//총 갯수 숨김
        document.getElementById('searchAddNoShow').style.visibility = 'hidden';//노쇼 추가 버튼 숨김
        document.getElementById('homeSearchNoShowExists').classList.add('none');//리스트 숨김
        document.getElementById('homeSearchNoShowContents').value = '';
    }

    let setVisiblity = function(elementId, show){
        let add = 'flex', remove = 'none';
        if(!show){
            add = 'none';
            remove = 'flex';
        }
        document.getElementById(elementId).classList.add(add);
        document.getElementById(elementId).classList.remove(remove);
    }
    document.getElementById("chkAgreeLabel").onclick = function(e){
        document.getElementById("chkAgree").click();
    }

    // home page events
    document.getElementById("homeSearchNoShow").onclick = function(e){
        setVisiblity('homePage', false);
        setVisiblity('homeSearchArea', true);
        history.pushState({link : "homeSearchArea"}, "", "/home/search");
        document.querySelector('body').style.background = "#f8f8f8";
        document.getElementById("homeSearchNoShowContents").focus();
    };

    document.getElementById("homeAddNoShow").onclick = function(e){
        setVisiblity('homePage', false);
        setVisiblity('homeAddNoShowArea', true);
        history.pushState({link: "homeAddNoShowArea"}, "", "/home/add");
        document.querySelector('body').style.background = "#f8f8f8";
    }

    document.querySelectorAll('div[name="addNoShowDirect"]').forEach((el) => {
        el.onclick = function(){
            setVisiblity('homeSearchArea', false);
            setVisiblity('homeAddNoShowArea', true);
            history.pushState({link: "homeAddNoShowArea"}, "", "/home/add");

            document.getElementById('telInAddNoShow').value = document.getElementById('homeSearchNoShowContents').value;
        }
    });

    //노쇼 등록 요청 버튼
    let noShowToSend = null;
    document.getElementById("btnAddNoShow").onclick = function(e){

        let str = document.getElementById('telInAddNoShow').value;
        let phone = ('' + str).replace(/\D/g, '');
        if(!phoneNumberValidation(phone)){
            alert(`전화번호가 올바르지 않습니다.(${str})`);
            return false;
        }
        str = document.getElementById('dateInAddNoShow').value;
        let date = str.replace(/-/gi, '');
        if(!dateValidation(date)){
            alert(`날짜가 올바르지 않습니다.(${str})`);
            return false;
        }

        let noShowCase = document.querySelector('input[name="noShowCase"]:checked').value;
        let noShowCaseEtc = document.getElementById('noShowCaseEtc').value;
        if(noShowCaseEtc && noShowCaseEtc.trim().length > 0){
            noShowCase = noShowCaseEtc;
        }

        noShowToSend = {
            phone: phone,
            noShowDate: date,
            noShowCase: noShowCase
        };

        setVisiblity('homeAddNoShowArea', false);
        setVisiblity('contractArea', true);
        history.pushState({link: "contractArea"}, "", "/home/contract");

    }


    //이용약관 동의 및 등록 버튼
    document.getElementById("btnAgreeAndAddNoShow").onclick = function(e){

        if(!document.querySelector('input[id="chkAgree"]:checked')){
            document.getElementById("chkAgree").click();
        }

        sendAjax('/add', noShowToSend, (res) => {
            setVisiblity('contractArea', false);
            if(res.status){
                setVisiblity('homeAddNoShowArea', true);
                history.pushState({link: "homeAddNoShowArea"}, "", "/home/add");
                setVisiblity('modalAddNoShowComplete', true);
            }else{
                alert(res.message);
                setVisiblity('homePage', true);
                history.pushState({link: "homePage"}, "", "/home");
                document.querySelector('body').style.background = "#334150";
            }
        });
    }

    document.getElementById("btnShare").onclick = function(e){
        const shareData = {
            title: 'WA:SHOW',
            text: '노쇼 블랙리스트 공유',
            url: 'https://www.washow.co.kr/home',
        }
        try {
            navigator.share(shareData);
        } catch(err) {
            console.log(JSON.stringify(err));
        }
    }

    document.getElementById("btnClose").onclick = function(e){
        resetAllInput();

        setVisiblity('modalAddNoShowComplete', false);
        setVisiblity('homeAddNoShowArea', false);
        setVisiblity('homePage', true);
        history.pushState({link: "homePage"}, "", "/home");
        document.querySelector('body').style.background = "#334150";
    }



    document.getElementsByName("goBackBtn").forEach((btn)=>{
        btn.onclick = function(e){
            history.back();
        }
    });

    document.getElementById("homeLinkToIndex").onclick = function(e){
        location.href = "/index";
    }

    addTelListener(document.getElementById("homeSearchNoShowContents"), ()=>{
        document.getElementById("homeSearchNoShowAction").click();
    });
    addTelListener(document.getElementById("telInAddNoShow"));


    document.getElementById("homeSearchNoShowAction").onclick = function(e){
        let str = document.getElementById('homeSearchNoShowContents').value;
        let cleaned = ('' + str).replace(/\D/g, '');
        searchNoShowAction(cleaned);


        document.getElementById("homeSearchNoShowContents").blur();
    }


    document.getElementById("homeSearchCloseTooltip").onclick = function(){
        document.querySelector("#homeSearchArea .page-bottom").classList.add('none');

        document.querySelector(".scrollArea").classList.add('scrollAreaWithBanner');
    }

    // search handler
    let searchNoShowAction = function (searchContents) {
        if (!searchContents || searchContents.trim() === "") {
            alert("검색할 전화번호를 입력해주세요.");
            return;
        } else if (!phoneNumberValidation(searchContents)) {
            alert(`전화번호가 올바르지 않습니다.(${searchContents})`);
            return;
        }

        sendAjax('/search', {contact: searchContents}, (data)=>{

            if(data.length === 0){// 노쇼 없음
                document.getElementById('searchAddNoShow').style.visibility = 'hidden'; // 노쇼 추가 숨김
                document.getElementById('homeSearchNoShowCountArea').style.visibility = 'hidden';//총 갯수 숨김
                document.getElementById('homeSearchNoShowExists').classList.add('none');//리스트 숨김
                document.getElementById('homeSearchNoShowNotExist').classList.remove('none');//이미지 보여줌
            }else{// 노쇼 있음
                document.getElementById('searchAddNoShow').style.visibility = 'visible'; // 노쇼 추가 보여줌
                document.getElementById('homeSearchNoShowCount').innerText = data.length;//총 갯수 넣어주기
                document.getElementById('homeSearchNoShowCountArea').style.visibility = 'visible';//총 갯수 보여줌
                let list = "";
                data.sort((a, b) => {
                    return b.date - a.date;
                });
                data.forEach(function(noShow){
                    list += "<div class='homeSearchNoShowData flex'><div class='homeSearchNoShowDate montserrat'>"+noShow.date.substring(0,4)+"."+noShow.date.substring(4,6)+"."+noShow.date.substring(6)+"</div><span style='margin:0 15px;opacity:0.5;color:#393535;'> |</span><div class='homeSearchNoShowCase'>"+ (noShow.noShowCase || '노쇼') +"</div></div>";
                });
                document.getElementById('homeSearchNoShowList').innerHTML = list;// 리스트 채움
                document.getElementById('homeSearchNoShowNotExist').classList.add('none');//이미지 숨김
                document.getElementById('homeSearchNoShowExists').classList.remove('none');//리스트 보여줌
            }
        });
    }

    let deferredPrompt = null;
    document.getElementById('btnInstall').onclick = function(e){
        if(deferredPrompt){
            deferredPrompt.prompt();
            deferredPrompt.userChoice.then(choiceResult => {
                if (choiceResult.outcome === 'accepted') {
                    console.log('User accepted the A2HS prompt');
                    document.getElementById('homeSearchCloseTooltip').click();
                } else {
                    console.log('User dismissed the A2HS prompt');
                }
                deferredPrompt = null;
            });
        }
    }

    window.addEventListener('beforeinstallprompt', e => {
        deferredPrompt = e;
    });

    // common history handler
    window.onpopstate = function(state){
        let targets = document.getElementsByClassName('page');
        for(let i=0;i<targets.length;i++){
            targets[i].classList.add("none");
            targets[i].classList.remove("flex");
        }
        let linkName = 'homePage';
        if(state && state.state && state.state.link){
            linkName = state.state.link;
        }
        if(linkName === 'homePage'){
            resetAllInput();
            document.querySelector('body').style.background = "#334150";
        }
        setVisiblity(linkName, true);

    }

    function getMobileOperatingSystem() {
        const isInStandaloneMode = 'standalone' in window.navigator && window.navigator.standalone;
        if(isInStandaloneMode){
            document.querySelector("#homeSearchArea .page-bottom").classList.add('none');
            return;
        }
        let userAgent = navigator.userAgent || navigator.vendor || window.opera;

        // Windows Phone must come first because its UA also contains "Android"
        if (/windows phone/i.test(userAgent)) {
            document.querySelector("#homeSearchArea .page-bottom").classList.add('none');
            return;
        }

        if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
            document.querySelector("#homeSearchArea .page-bottom").classList.remove('none');
            document.querySelector(".homeSearchAddToDesktop.ios").classList.remove('none');
            document.querySelector(".homeSearchAddToDesktop.ios").classList.add('flex');
            document.querySelector(".homeSearchAddToDesktop.android").classList.remove('flex');
            document.querySelector(".homeSearchAddToDesktop.android").classList.add('none');
            return;
        }else{
            document.querySelector("#homeSearchArea .page-bottom").classList.remove('none');
            document.querySelector(".homeSearchAddToDesktop.android").classList.remove('none');
            document.querySelector(".homeSearchAddToDesktop.android").classList.add('flex');
            document.querySelector(".homeSearchAddToDesktop.ios").classList.remove('flex');
            document.querySelector(".homeSearchAddToDesktop.ios").classList.add('none');
        }


    }
    getMobileOperatingSystem();

</script>
