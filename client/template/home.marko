      <div class="homeSearchAddToDesktop android none cursor-pointer flex-auto">
        <img src="/nmns/img/washow_icon.svg" alt="WA:SHOW"/>
        <div class="flex-auto" style="margin-left:12px">
          <span>홈 화면에 </span><img src="/nmns/img/BI_black.svg" alt="WA:SHOW"/> 추가
        </div>
      </div>
      <div class="homeSearchAddToDesktop ios none cursor-pointer flex-auto relative">
        <img src="/nmns/img/ios_share.svg" alt="홈화면에 추가"/>
        <div class="flex-auto col" style="margin-left:17px">
          이 버튼을 눌러 <span class="bold">&#39;홈 화면에 추가&#39;</span><br/>바로가기를 설치하면 편리해요!
        </div>
        <div class="homeSearchAddToDesktopTooltipTip"></div>
      </div>
      <div id="homeSearchCloseTooltip" class="right cursor-pointer">
        <span aria-hidden="true">&times;</span>
      </div>
    </div>
  </div>
</section>
<section id="homeAddNoShowArea" class="page none">
노쇼 추가하기를 누르셨군요.
</section>

<script>
  // home page events
  document.getElementById("homeSearchNoShow").onclick = function(e){
    document.getElementById("homePage").classList.add("none");//기본 화면을 없앤다.
    document.getElementById("homePage").classList.remove("flex");//기본 화면을 없앤다.
    document.getElementById("homeSearchArea").classList.add("flex");//검색화면을 보여준다.
    document.getElementById("homeSearchArea").classList.remove("none");//검색화면을 보여준다.
    history.pushState({link : "homeSearchArea"}, "", "/home/search");
    document.getElementById("homeSearchNoShowContents").focus();
  };
  
  document.getElementById("homeAddNoShow").onclick = function(e){
    document.getElementById("homePage").classList.add("none");//기본 화면을 없앤다.
    document.getElementById("homePage").classList.remove("flex");//기본 화면을 없앤다.
    document.getElementById("homeAddNoShowArea").classList.add("flex");//노쇼추가화면을 보여준다.
    document.getElementById("homeAddNoShowArea").classList.remove("none");//노쇼추가화면을 보여준다.
    history.pushState({link: "homeAddNoShowArea"}, "", "/home/add");
  }

  document.getElementById("goBackBtn").onclick = function(e){
    document.getElementById('homeSearchNoShowNotExist').classList.add('none');//이미지 숨김
    document.getElementById('homeSearchNoShowCountArea').style.visibility = 'hidden';//총 갯수 숨김
    document.getElementById('homeSearchNoShowExists').classList.add('none');//리스트 숨김
    document.getElementById('homeSearchNoShowContents').value = '';
    history.back();
  }

  document.getElementById("homeLinkToIndex").onclick = function(e){
    location.href = "/index";
  }
  
  // search page events
  document.getElementById("homeSearchNoShowContents").onkeypress = function(e){
    if(isNaN(parseInt(e.key))){
      e.returnValue=false;
    }
  }
  document.getElementById("homeSearchNoShowContents").onkeyup = function(e){
    if(e.key === "Enter"){
      document.getElementById("homeSearchNoShowAction").click();
    }else{
      let str = document.getElementById('homeSearchNoShowContents').value;
      let cleaned = ('' + str).replace(/\D/g, '');
      let formatted = '';
      let length = cleaned.length;
      if(length <= 3){
        return;
      }
      if(length > 3 && length <= 6){
        formatted = cleaned.substring(0,3) + '-' + cleaned.substring(3);
      }else if(length > 6 && length <= 10 ){
        formatted = cleaned.substring(0,3) + '-' + cleaned.substring(3, 6) + '-' + cleaned.substring(6);
      }else{
        formatted = cleaned.substring(0,3) + '-' + cleaned.substring(3, 7) + '-' + cleaned.substring(7);
      }
      document.getElementById('homeSearchNoShowContents').value =  formatted;
    }
  }

  document.getElementById("homeSearchNoShowAction").onclick = function(e){
    let str = document.getElementById('homeSearchNoShowContents').value;
    let cleaned = ('' + str).replace(/\D/g, '');
    searchNoShowAction(cleaned);
  }

  document.getElementById("homeSearchCloseTooltip").onclick = function(){
    document.getElementById('homeSearchNoShowExists').classList.add('withoutTooltip');
    document.querySelector("#homeSearchArea .page-bottom").classList.add('none');
  }
  // search handler
  function searchNoShowAction(searchContents){
    if(!searchContents || searchContents.trim() === ""){
      alert("검색할 고객의 전화번호를 입력해주세요.");
      return;
    } else if(searchContents.length !== 10 && searchContents.length !== 11){
      alert("검색할 고객의 전화번호를 정확히 입력해주세요.");
      return;
    }

    // request to server
    if(Math.random() * 10 < 5){
      handleNoShowSearchResponse([]);
    }else{
      var response = [];
      var size = Math.random() * 10 + 1;
      var index = 0;
      while(index < size){
        response.push({date: '201911' + (30 - index), noShowCase:'진상이었나봐요.'});
        index++;
      }
      handleNoShowSearchResponse(response);
    }
  }

  function handleNoShowSearchResponse(data){
    if(data.length === 0){// 노쇼 없음
      document.getElementById('homeSearchNoShowCountArea').style.visibility = 'hidden';//총 갯수 숨김
      document.getElementById('homeSearchNoShowExists').classList.add('none');//리스트 숨김
      document.getElementById('homeSearchNoShowNotExist').classList.remove('none');//이미지 보여줌
    }else{// 노쇼 있음
      document.getElementById('homeSearchNoShowCount').innerText = data.length;//총 갯수 넣어주기
      document.getElementById('homeSearchNoShowCountArea').style.visibility = 'visible';//총 갯수 보여줌
      var list = "";
      data.forEach(function(noShow){
        list += drawNoShowList(noShow);
      });
      document.getElementById('homeSearchNoShowList').innerHTML = list;// 리스트 채움
      document.getElementById('homeSearchNoShowNotExist').classList.add('none');//이미지 숨김
      document.getElementById('homeSearchNoShowExists').classList.remove('none');//리스트 보여줌
    }
  }

  function drawNoShowList(noShow){
    return "<div class='homeSearchNoShowData flex'><div class='homeSearchNoShowDate montserrat'>"+noShow.date.substring(0,4)+"."+noShow.date.substring(4,6)+"."+noShow.date.substring(6)+"</div><span style='margin:0 15px;opacity:0.7'> |</span><div class='homeSearchNoShowCase'>"+noShow.noShowCase+"</div></div>";
  }

  // common history handler
  window.onpopstate = function(state){
    var targets = document.getElementsByClassName('page');
    for(var i=0;i<targets.length;i++){
      targets[i].classList.add("none");
      targets[i].classList.remove("flex");
    }
    if(state === null || state.state === null || state.state.link === "homePage"){//홈페이지로 돌아옴
      document.getElementById("homePage").classList.add("flex");//기본 화면을 보여준다.
      document.getElementById("homePage").classList.remove("none");//기본 화면을 보여준다.
    }else{
      document.getElementById(state.state.link).classList.add("flex");//화면을 보여준다.
      document.getElementById(state.state.link).classList.remove("none");//화면을 보여준다.
    }
    
  }

  function getMobileOperatingSystem() {
    var userAgent = navigator.userAgent || navigator.vendor || window.opera;

      // Windows Phone must come first because its UA also contains "Android"
      if (/windows phone/i.test(userAgent)) {
        document.querySelector("#homeSearchArea .page-bottom").classList.add('none');
        return;
      }

      if (/android/i.test(userAgent)) {
        document.querySelector("#homeSearchArea .page-bottom").classList.remove('none');
        document.querySelector(".homeSearchAddToDesktop.android").classList.remove('none');
        document.querySelector(".homeSearchAddToDesktop.android").classList.add('flex');
        document.querySelector(".homeSearchAddToDesktop.ios").classList.remove('flex');
        document.querySelector(".homeSearchAddToDesktop.ios").classList.add('none');
        return;
      }

      if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
        document.querySelector("#homeSearchArea .page-bottom").classList.remove('none');
        document.querySelector(".homeSearchAddToDesktop.ios").classList.remove('none');
        document.querySelector(".homeSearchAddToDesktop.ios").classList.add('flex');
        document.querySelector(".homeSearchAddToDesktop.android").classList.remove('flex');
        document.querySelector(".homeSearchAddToDesktop.android").classList.add('none');
        return;
      }

      document.querySelector("#homeSearchArea .page-bottom").classList.add('none');
  }
  getMobileOperatingSystem();
</script>
</body>

</html>