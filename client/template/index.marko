<script>
  var section = [false, false, false, false, false];
  function isScrolledIntoView(el) {
      var rect = el.getBoundingClientRect();
      return rect.top < window.innerHeight && rect.bottom >= 0;
  }
  window.onscroll = detectScroll;
  function detectScroll(){
    var i=0;
    while(i < section.length){
      if(!section[i] && isScrolledIntoView(document.getElementById('section' + (i+2)))){
        section[i] = true;
        document.getElementById('section' + (i+2)).style['-webkit-animation-play-state'] = 'running';
        document.getElementById('section' + (i+2)).style['animation-play-state'] = 'running';
      }
      i++;
    }
  }
  detectScroll();
  var goTo = function(index){
    if(siema.currentSlide !== index){
      if(siema.currentSlide > index){
        siema.prev(siema.currentSlide - index);
      }else{
        siema.next(index - siema.currentSlide);
      }
