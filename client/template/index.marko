                             email이 있는 경우는 sns_signin post 요청을 한다.
                             snsLinkId: res.id
                             snsType: KAKAO
                             snsEmail: res.kakao_account.email
                            **/
							if(!res.kakao_account.email || !res.kakao_account.email){
								alert('이메일 주소는 연동 및 로그인에 필수로 필요합니다. 카카오톡>설정>프라이버시>카카오 계정>연결 서비스 관리 메뉴에서 연결을 해제 후 다시 시도해주세요.');
								Kakao.Auth.logout();
							}else{
								var xhr = new XMLHttpRequest();
								xhr.open('POST', '/sns_signin');
								xhr.setRequestHeader('Content-Type', 'application/json');
								xhr.onload = function() {
									if (xhr && xhr.status === 200) {
										location.href = '/';
									} else {
										alert("카카오톡 로그인에 실패했습니다. 다시 시도해주세요!");
									}
								};
								xhr.send(JSON.stringify({
									snsLinkId: res.id,
									snsType: 'KAKAO',
									snsEmail: res.kakao_account.email
								}));
							}
						}, fail: function(error){
							alert('카카오 서버와 연결하지 못했습니다. 다시 시도해주세요.');
						}})
					},
					fail: function(err) {
						alert('연결이 취소되었습니다.');
						console.log(JSON.stringify(err));
					}
				});
			});
		};
		
		function submitResetPassword(e){
			e.stopPropagation();
			if(!/^(([^<>()\[\]\.,;:\s@\"]+(\.[^<>()\[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i.test(document.getElementById("resetPasswordEmail").value)){
				alert('이메일을 올바르게 입력해주세요.');
				return false;
			}
			var xhr = new XMLHttpRequest();
			xhr.open('POST', '/resetPassword');
			xhr.setRequestHeader('Content-Type', 'application/json');
			xhr.onload = function() {
				if (xhr && xhr.status === 200) {
					alert("비밀번호 초기화 메일을 보냈습니다. 메일을 확인해주세요!");
				} else if (xhr && xhr.status === 404) {
					alert("등록되지 않은 이메일입니다. 이메일 주소를 확인해주세요!");
				} else {
					alert("메일 보내기에 실패했습니다. 다시 시도해주세요!");
				}
			};
			xhr.send(JSON.stringify({
				email: document.getElementById('resetPasswordEmail').value
			}));
		}
		addEvent(document.getElementById('resetPassword'), 'click', submitResetPassword);
		addEvent(document.getElementById('resetPasswordEmail'), 'keyup', function(e){
			if(e.which === 13){
				submitResetPassword(e);
			}
		});
		
		addEvent(document.getElementById("agreeContract"), "click", function(e){
			e.stopPropagation();
			document.getElementById('signupForm').contract.checked = true;
			showModal('signupModal')(e);
		});
		addEvent(document.getElementById("disagreeContract"), "click", function(e){
			e.stopPropagation();
			document.getElementById('signupForm').contract.checked = false;
		});
		
		addEvent(document.getElementById('addLogo'), 'click', function(e){
			e.preventDefault();
			e.stopPropagation();
			document.getElementById('logo').click();
			return false;
		});
		addEvent(document.getElementById('signupForm').logoFileName, 'click', function(e){
			e.preventDefault();
			e.stopPropagation();
			document.getElementById('logo').click();
			return false;
		});
		addEvent(document.getElementById('logo'), 'change', function() {
			var file = this.files[0];
			if(file){
				var img = document.createElement('img');
				img.onload = function(){
					if(img.naturalWidth <= 130 && img.naturalHeight <= 130){
						document.getElementById('signupForm').logoFileName.value = file.name;
					}else{
						alert('이미지의 크기가 130 X 130보다 큽니다.\n작은 이미지로 올려주세요.');
						document.getElementById('logo').reset? document.getElementById('logo').reset() : document.getElementById('logo').removeAttribute('value');
					}
					img.remove();
				}
				img.setAttribute('src', (window.URL || window.webkitURL).createObjectURL(file));
			}
		});
		addEvent(document.getElementById('signupBtn'), "click", function(e){
			e.stopPropagation();
			if(!document.getElementById('contract').checked){
				alert('이용약관의 동의는 필수입니다.');
				return;
			}
			var form = document.getElementById('signupForm');
			if(!/^(([^<>()\[\]\.,;:\s@\"]+(\.[^<>()\[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i.test(form.email.value)){
				alert('이메일을 올바르게 입력해주세요.');
				form.email.focus();
				return false;
			}
			if (!/\W+/.test(form.password.value) || !/[0-9]+/.test(form.password.value)) {
					alert("비밀번호는 하나 이상의 숫자와 특수문자를 포함해야합니다.");
					form.password.focus();
					return;
			}
			if (form.password.value !== form.repassword.value) {
					alert("비밀번호 확인 값이 일치하지 않습니다.");
					form.repassword.focus();
					return;
			}
			var xhr = new XMLHttpRequest();
			xhr.open('POST', '/signup');
			//xhr.setRequestHeader('Content-Type', 'multipart/form-data');
			xhr.onload = function() {
				try{
					 if (xhr && xhr.status !== 200) {
							alert(response.message || '회원가입에 실패했습니다. 다시 시도해주세요.');
							return;
					 } else {
						var response = JSON.parse(xhr.response);
						if(response.status !== 200){
							alert(response.message || '회원가입에 실패했습니다. 다시 시도해주세요.');
							return;
						}
						showModal('emailModal')();
						document.getElementById('signinForm').email.value = form.email.value;
						document.getElementById('signupForm').reset();
					 }
				}catch(e){
					alert('회원가입에 실패했습니다. 다시 시도해주세요.');
				}				
			};
			xhr.send(new FormData(document.getElementById('signupForm')));
		});
		addEvent(document.getElementById('resendEmail'), 'click', function(e){
			e.stopPropagation();
			if(document.getElementById('signinForm').email.value){
				var xhr = new XMLHttpRequest();
				xhr.open('POST', '/sendVerification');
				xhr.setRequestHeader('Content-Type', 'application/json');
				xhr.onload = function() {
					if (xhr && xhr.status === 200) {
						alert("인증 메일을 다시 보냈습니다. 메일을 확인해주세요!");
					} else if (xhr && xhr.status === 404) {
						alert("등록되지 않은 이메일입니다. 이메일 주소를 확인해주세요!");
					} else {
						alert("메일 보내기에 실패했습니다. 다시 시도해주세요!");
					}
				};
				xhr.send(JSON.stringify({
					email: document.getElementById('signinForm').email.value
				}));
			}else{
				alert('이메일 정보가 없습니다. 로그인 후 내 계정정보 > 인증메일 재전송 기능을 이용해주세요.');
			}
		});
		addEvent(document.getElementById('completeAuth'), 'click', function(e){
			e.stopPropagation();
			showModal('signinModal')();
		});
		if (navigator.appVersion.indexOf("Win")!=-1){
			new PerfectScrollbar("#signupForm");
		}
		if(!!${input.authRequired}){
			showModal('emailModal')();
		}
  })();
  window.fitText(document.getElementById('caption1'));
  window.fitText(document.getElementById('caption2'));
</script>
